{
	"name": "coreback",
	"version": "1.0.0",
	"description": "Este proyecto fue engendrado con fastify cli y luego clonado del repo de microservicios",
	"main": "server.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
		"start": "fastify start -l info dist/server.js",
		"build:ts": "tsc",
		"build": "tsc",
		"dev": "ts-node-dev src/server.ts",
		"lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .eslintignore .",
		"lint:prettier": "prettier --check .",
		"lintfix": "prettier --write --list-different . && yarn lint --fix",
		"prepare": "husky install",
		"release": "standard-version"
	},
	"lint-staged": {
		"*.**": "prettier --check --ignore-unknown"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@commitlint/cli": "^16.2.3",
		"@commitlint/config-conventional": "^16.2.1",
		"fastify": "^3.0.0",
		"fastify-autoload": "^3.10.0",
		"fastify-cli": "^2.15.0",
		"fastify-plugin": "^3.0.0",
		"fastify-routes": "^3.1.0",
		"fastify-sensible": "^3.1.2",
		"fastify-xml-body-parser": "^2.1.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7"
	},
	"devDependencies": {
		"@types/node": "^17.0.8",
		"@types/tap": "^15.0.5",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"concurrently": "^7.0.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"fastify-tsconfig": "^1.0.1",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.1",
		"standard-version": "^9.3.2",
		"tap": "^15.1.6",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.5.4"
	}
}
