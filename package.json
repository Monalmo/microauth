{
	"name": "coreback",
	"version": "1.0.0",
	"description": "Este proyecto fue engendrado con fastify cli y luego clonado del repo de microservicios",
	"main": "server.ts",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"dev2": "ts-node-dev  --exit-child src/server.ts",
		"dev": "MODO=dev tsnd --respawn -r tsconfig-paths/register src/server.ts",
		"build": "yarn clean && tsc && tsc-alias && yarn copy-files",
		"clean": "rm -rf ./build",
		"copy-files": "cp ./src/favicon.svg ./build/favicon.svg",
		"start": "node build/server.js",
		"lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .eslintignore .",
		"lint:prettier": "prettier --check .",
		"lintfix": "prettier --write --list-different . && yarn lint --fix",
		"prepare": "husky install",
		"release": "standard-version"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^4.0.1",
		"@fastify/cors": "^7.0.0",
		"@fastify/helmet": "^8.0.1",
		"@fastify/routes": "^4.0.0",
		"@fastify/sensible": "^4.1.0",
		"@sinclair/typebox": "^0.23.5",
		"axios": "^0.26.0",
		"bcrypt": "^5.0.1",
		"chalk": "^4.1.2",
		"dayjs": "^1.11.1",
		"fastify": "^3.25.0",
		"fastify-cli": "^2.15.0",
		"fastify-plugin": "^3.0.0",
		"fastify-xml-body-parser": "^2.1.0",
		"jose": "^4.8.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"moment-range": "^4.0.2",
		"moment-timezone": "^0.5.34",
		"mongodb": "^4.2.2",
		"nanoid": "^3.3.4",
		"network-config": "^3.0.0",
		"phone": "^3.1.17",
		"rut.js": "^2.1.0",
		"ts-node": "^10.7.0",
		"validator": "^13.7.0"
	},
	"optionalDependencies": {
		"node-network-manager": "^1.0.9"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@types/lodash": "^4.14.178",
		"@types/node": "^17.0.5",
		"@types/pino": "^7.0.5",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"babel-eslint": "^10.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-promise": "^4.3.1",
		"eslint-plugin-standard": "^4.1.0",
		"husky": "^7.0.4",
		"json-schema-to-typescript": "^10.1.5",
		"pino": "^7.6.1",
		"pino-pretty": "^7.3.0",
		"prettier": "^2.6.2",
		"ts-node-dev": "^1.1.8",
		"tsc-alias": "^1.6.3",
		"tsconfig-paths": "^3.12.0",
		"typescript": "^4.5.4"
	},
	"packageManager": "yarn@3.2.1"
}
